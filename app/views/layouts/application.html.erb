    <script type="text/javascript" src="process.js"></script>
    <%= favicon_link_tag '/images/logo-transparent.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '144x144' %>
    <%= favicon_link_tag '/images/logo-transparent.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '114x114' %>
