    <%= favicon_link_tag '/images/logo-transparent.png', :rel => 'shortcut icon' %>
    <script>
      // Verify if store is already in firestore
      <% if current_store  %>
        var sessions_count = <%= current_store.sessions_count %>;
        if (sessions_count === 1) {
          var data = {
            "type": 'new-store'
          };
          addDataToFirestore(data)
        }
        readDataFromFirestore({ data: { "type": "none" } });
      <% end %>
    </script>
    <%= csrf_meta_tags %>
  </head>
  <body id="body">
    <%= render 'layouts/modals' %>
    <%= render navbar %>
    <%= render body %>
    <%= render footer %>
  </body>
</html>
